#include<iostream>
#include<algorithm>
using namespace std;

int main(void){
    int num;
    cin >> num;
    
    int arr[10] = {0};
    
    while(true){
    
        arr[n%10]++;
        if(n/10 == 0 ) break;
        n/=10;
        
    }
    
    int num2 =0;
    for( int i=0;i<10;i++) {
    
        if(i!=9 && i != 6) num2 = max(num2, arr[i]);
    }
    
    cout << max(num2, (arr[6] + arr[9] +1) /2);
